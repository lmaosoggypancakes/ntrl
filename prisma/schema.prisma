generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Park {
  id              Int   @default(autoincrement()) @id
  title           String 
  dogabble        Boolean
  activities      String[] 
  transportation  String[]
  region          Region
  utilities       String[]
  x               Float
  y               Float
  visits          Int   @default(0)
}

enum Region {
  Western 
  Southern  
  Northern
}